+			gpios = <&gpio0 0x1e GPIO_ACTIVE_HIGH>;
+			label = "red tx LED";
+		};
+		radio_led_rx: led_rx {
+			gpios = <&gpio0 0x1f GPIO_ACTIVE_HIGH>;
