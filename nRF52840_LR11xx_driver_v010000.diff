+/*
+ * Copyright (c) 2023 Nordic Semiconductor ASA
+ *
+ * SPDX-License-Identifier: LicenseRef-Nordic-5-Clause
+ */
+
+#include <stdio.h>
+#include <sid_api.h>
+#include <sid_error.h>
+#include <sid_hal_reset_ifc.h>
+#include <sid_clock_ifc.h>
